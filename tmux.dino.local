#!/bin/zsh

DINO_SESSION="dino"
# Create dotfiles session
tmux new-session -s ${DINO_SESSION} -n vim -d

# Vim (1)
tmux send-keys -t ${DINO_SESSION} "cd ~/ultimatetimmah/dino && vim" C-m

# Git (2)
tmux new-window -n git -c ~/ultimatetimmah/dino -t ${DINO_SESSION}
tmux send-keys -t ${DINO_SESSION}:2 'git status' C-m

# Set default window
tmux select-window -t ${DINO_SESSION}:1
